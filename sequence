# COMET special parameters
# ISO linear units : m, m/s, m/s�, V, A, s (used in case of interpolation)
# ISO rotary units : t, t/s, t/s�, V, A, s

#include <math.h>

void func0(void){
	RST.0;
	PWR.0 = 1;
	IND.0;
	MVE.0=0.0035,0.0014,0.001;
}

void func1(void){
	float T = 7;
	float amp = 5;
	float t = T / 4;
	float d = 2 * amp;
	float v = d / t;
	float a = v / t;
	float pos = ML7.0;
	pos += d;
	X1.0 = iconv(0.002, _upi);
	X1.1 = iconv(v, _usi);
	X1.2 = iconv(a, _uai);
	MVE.0 = X1.0,X1.1,X1.2; 
	MVE.0 = ML7.0,X1.1,X1.2;
}